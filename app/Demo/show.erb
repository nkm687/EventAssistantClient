
<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @demo.name %>
    <script src="/public/star/jquery.js" type="text/javascript"></script>
    <script src="/public/star/jquery.MetaData.js" type="text/javascript" language="javascript"></script>
    <script src="/public/star/jquery.rating.js" type="text/javascript" language="javascript"></script>
    <link href="/public/star/jquery.rating.css" type="text/css" rel="stylesheet"/>
    <link type="text/css" href="/@/js/jquery/ui/jquery.ui.css" rel="stylesheet" />

    </h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
   
  </div>

  <div data-role="content">
      
      <form method="POST" id="showDemo" action="<%= url_for :action => :updateRating %>">
      <script>
             $(function(){
             $('.auto-submit-star').rating({
               callback: function(value, link){
                  alert(value);
                  document.getElementById('userRatingValue').value = value;
                  document.getElementById('showDemo').submit();
                            }
                    });
                });
            </script>
            
      <input type="hidden" name="id" value="<%= @demo.object %>"/>
      <ul data-role="listview" style="border:0px">
      
              <!--  <li style="height:60px;border:0px">
                         <div class="itemValue" style="margin-left:60px;align-self: inherit;text-align:center;font-size:30px;">App Monitor
                         </div>
                 </li> -->
      
       
          
              <li style="margin-top:20px;border:0px;height:120px">
            
                 <div class="itemValue" style="margin-left:60px;margin-right:0px;font-size:20px;text-align:left;"><%= @demo.description %>
                 </div>
               </li>  
      
                 <li style="margin-top:20px;border:0px;margin-left:10px">
                    <div class="itemLabel">AvgUserRating <%= @demo.avg_user_rating %> 
                    </div>
                </li>
     <input type="hidden" id="userRatingValue" name="userRatingValue" value=""  />
     
      <li style="margin-top:20px;border:0px;margin-left:10px;height:40px">
                         <div class="itemLabel">Contributors <%= @demo.contributers %> 
                         </div>
                     </li>
     
      <li style="margin-top:10px;border:0px">
               
                   <table width="100%" cellspacing="10" style="margin-left:0px"> <tr>
                   <td width="30%" >Your Rate:</td>
                     <td valign="top" width="" >
                   <div class="Clear">
                      
                       <input class="auto-submit-star" type="radio" name="test-3A-rating-1" value="1" checked="checked"/>
                       <input class="auto-submit-star" type="radio" name="test-3A-rating-1" value="2" />
                       <input class="auto-submit-star" type="radio" name="test-3A-rating-1" value="3"/>
                       <input class="auto-submit-star" type="radio" name="test-3A-rating-1" value="4"/>
                       <input class="auto-submit-star" type="radio" name="test-3A-rating-1" value="5"/>
                    </div>
                     
                     </td>
                    
                    </tr>
                   </table>
                 </li>
                 


 
<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @demo.avg_user_rating %><script src="/public/star/jquery.js" type="text/javascript"></script>
<script src="/public/star/jquery.MetaData.js" type="text/javascript" language="javascript"></script>
 <script src="/public/star/jquery.rating.js" type="text/javascript" language="javascript"></script>
 <link href="/public/star/jquery.rating.css" type="text/css" rel="stylesheet"/>
<link type="text/css" href="/@/js/jquery/ui/jquery.ui.css" rel="stylesheet" />
<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.1/jquery-ui.min.js" type="text/javascript"></script></h1>
    <a href="<%= url_for :action => :show, :id => @demo.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @demo.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @demo.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="demo[avg_user_rating]" class="fieldLabel">Avg user rating</label>
            <input type="text" id="demo[avg_user_rating]" name="demo[avg_user_rating]" value="<%= @demo.avg_user_rating %>" <%= placeholder( "Avg user rating" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="demo[description]" class="fieldLabel">Description</label>
            <input type="text" id="demo[description]" name="demo[description]" value="<%= @demo.description %>" <%= placeholder( "Description" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="demo[keywords]" class="fieldLabel">Keywords</label>
            <input type="text" id="demo[keywords]" name="demo[keywords]" value="<%= @demo.keywords %>" <%= placeholder( "Keywords" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="demo[misc]" class="fieldLabel">Misc</label>
            <input type="text" id="demo[misc]" name="demo[misc]" value="<%= @demo.misc %>" <%= placeholder( "Misc" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="demo[name]" class="fieldLabel">Name</label>
            <input type="text" id="demo[name]" name="demo[name]" value="<%= @demo.name %>" <%= placeholder( "Name" ) %> />
          </div>
          
              <div data-role="fieldcontain">
                <label class="fieldLabel">rating</label>
                <input type="text" id="rating" name="rating" value="" <%= placeholder( "rating" ) %> />
              </div>
      
              
      <input type="submit" value="Update"/>
    </form>
    
  </div>


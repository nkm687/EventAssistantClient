<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @presenter.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @presenter.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="presenter[name]" class="fieldLabel">Name</label>
            <input type="text" id="presenter[name]" name="presenter[name]" <%= placeholder("Name") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="presenter[team]" class="fieldLabel">Team</label>
            <input type="text" id="presenter[team]" name="presenter[team]" <%= placeholder("Team") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
